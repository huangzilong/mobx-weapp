{
  "name": "mobx-weapp",
  "version": "0.1.3",
  "description": "Wechat App binding for Mobx",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src/ --out-dir dist/",
    "watch-compile": "npm run build -- --watch",
    "watch": "npm run watch-compile",
    "test": "jest --coverage"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jkzing/mobx-weapp.git"
  },
  "keywords": [
    "Mobx",
    "WeChat"
  ],
  "author": "jkzing <jingkai.zhao@foxmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkzing/mobx-weapp/issues"
  },
  "homepage": "https://github.com/jkzing/mobx-weapp#readme",
  "peerDependencies": {
    "mobx": "^3.1.9"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "mobx": "^3.1.9"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      ".node/",
      "test/"
    ]
  }
}
